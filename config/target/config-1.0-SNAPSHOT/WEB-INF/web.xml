<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>emp1</servlet-name>
        <servlet-class>com.example.config.HelloServlet</servlet-class>
        <init-param>
            <param-name>name</param-name>
            <param-value>harsha</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>emp1</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>emp2</servlet-name>
        <servlet-class>com.example.config.Emp</servlet-class>
        <init-param>
            <param-name>name2</param-name>
            <param-value>subhi</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>emp2</servlet-name>
        <url-pattern>/hello2</url-pattern>
    </servlet-mapping>
    <context-param>
        <param-name>name1</param-name>
        <param-value>Togaru</param-value>
    </context-param>
</web-app>